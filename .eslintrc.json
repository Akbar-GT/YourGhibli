{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi":"off",
        "indent":"off",
        "space-before-function-paren":"off",
        "accessor-pairs":"off"
    }
}
